{
  "name": "Free@Home ecowitt server",
  "version": "0.0.4",
  "id": "at.kronberger.freeathome.ecowitt-server",
  "license": "EUPL",
  "description": "Basic REST Endpoint with express, receiving POST requests from a weather station, using ecowitt protocol.",
  "url": "https://www.busch-jaeger.de",
  "author": "Christoph Kronberger",
  "type": "app",
  "entryPoint": "build/main.js",
  "parameters": {
    "default": {
      "name": "Settings",
      "items": {
        "username": {
          "name": "Username",
          "type": "string"
        },
        "password": {
          "name": "Password",
          "type": "password"
        },
        "ignoreReachability": {
          "name": "Ignore reachability",
          "type": "boolean"
        },
        "port": {
          "name": "Port",
          "type": "number",
          "min": 1024,
          "max": 65535
        }
      }
    }
  }
}
